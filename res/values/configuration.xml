<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- 
		{0} - latitude 
		{1} - longitude 
	-->
	<item name="googleMapsUrlTemplate" type="string">http://maps.google.com/maps/geo?output=xml&amp;ll={0},{1}</item>
	 
	<!-- Cache expiration time for products and categories in milliseconds -->
	<item name="cacheExpirationTime" type="string">300000</item>
	<!-- Size of categories cache in bytes --> 
	<item name="cacheCategorySize" type="string">50000</item> 
	<!-- Size of products cache in bytes --> 
	<item name="cacheProductSize" type="string">50000</item>
	<!-- Cache expiration time for images in milliseconds (1day : 86400000) -->
	<item name="cacheImageExpirationTime" type="string">86400000</item>
	<!-- Size of image cache in bytes --> 
	<item name="cacheImageSize" type="string">2000000</item>
	<item name="maxZoomPix" type="string">2000000</item>
	
	<!-- GPS -->
	<!-- Location time update in milliseconds -->
    <item name="locationTimeUpdate" type="string">1000</item>
	<!-- Location minimal distance update in meters -->
    <item name="locationMinimalDistance" type="string">1</item>
</resources>
